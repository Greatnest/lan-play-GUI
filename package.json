{
  "name": "lan-play-GUI",
  "version": "1.1.0",
  "description": "A GUI for lan-play",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "distWin": "electron-builder --ia32 --x64 -w",
    "distLin": "electron-builder --linux",
    "distMac": "electron-builder -o",
    "distPi": "electron-builder --linux --armv7l"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takashi1kun/lan-play-GUI.git"
  },
  "author": "Aitor Rosell Torralba<aitor.rosell.torralba@gmail.com>",
  "license": "GPL-3.0",
  "build": {
    "appId": "com.lanplayservermanager.app",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  },
  "dependencies": {
    "asar": "^0.14.6",
    "bootstrap": "^4.1.3",
    "child_process": "^1.0.2",
    "electron-json-config": "^1.5.3",
    "electron-packager": "^13.0.1",
    "jquery": "^3.3.1",
    "network-interfaces": "^1.1.0",
    "popper.js": "^1.14.6",
    "q": "^1.5.1",
    "request": "^2.88.0",
    "tcpdump": "0.0.1"
  }
}
